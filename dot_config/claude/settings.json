{
	"env": {
		"ENABLE_TOOL_SEARCH": "true"
	},
	"permisions": {
		"deny": [
			"Bash(rm -rf /*)",
			"Bash(rm -rf /)",
			"Bash(sudo rm -rf /*)",
			"Bash(sudo rm -rf /)",
			"Bash(dd if=/dev/zero of=/dev/*)",
			"Bash(dd if=/dev/random of=/dev/*)",
			"Bash(mkfs*)",
			"Bash(fdisk*)",
			"Bash(parted*)",
			"Bash(:(){ :|:& };:*)",
			"Bash(*fork*bomb*)",
			"Bash(> /dev/sda*)",
			"Bash(> /dev/nvme*)",
			"Bash(> /dev/sd*)",
			"Bash(cat /dev/urandom > *)",
			"Bash(* | nc -l*)",
			"Bash(* | netcat -l*)",
			"Bash(* | socat *)",
			"Bash(* | base64 -d | sh*)",
			"Bash(* | base64 -d | bash*)",
			"Bash(curl * | sh*)",
			"Bash(curl * | bash*)",
			"Bash(wget * | sh*)",
			"Bash(wget * | bash*)"
		]
	}
}
